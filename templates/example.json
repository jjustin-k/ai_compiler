{
    "inputs": {
      "x": {
        "shape": [1, 4, 4]
      }
    },
    "weights": {
      "w1": {
        "shape": [8, 4],
        "init": "random"
      },
      "b1": {
        "shape": [8],
        "init": "zeros"
      }
    },
    "nodes": [
      {
        "name": "maxpool1",
        "op": "maxpool2d",
        "inputs": ["x"],
        "params": {
          "kernel_size": [2, 2],
          "stride": [2, 2]
        }
      },
      {
        "name": "flatten",
        "op": "reshape",
        "inputs": ["maxpool1"],
        "params": {
          "shape": [1, 4]
        }
      },
      {
        "name": "fc1",
        "op": "matmul",
        "inputs": ["flatten", "w1"]
      },
      {
        "name": "add1",
        "op": "add",
        "inputs": ["fc1", "b1"]
      },
      {
        "name": "relu1",
        "op": "relu",
        "inputs": ["add1"]
      },
      {
        "name": "out",
        "op": "sub",
        "inputs": ["relu1", "b1"]
      }
    ],
    "outputs": ["out"]
  }
  